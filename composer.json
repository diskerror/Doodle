{
  "name": "diskerror/doodle",
  "description": "A place for my stuff",
  "license": "MIT",
  "type": "project",
  "homepage": "https://rawsound.com",
  "prefer-stable": true,
  "repositories": [
    {
      "type": "path",
      "url": "../Typed",
      "canonical": false
    },
    {
      "type": "vcs",
      "url": "../Autoload",
      "canonical": false
    },
    {
      "type":      "vcs",
      "url":       "https://github.com/phalcon/phalcon-devtools",
      "canonical": false
    }
  ],
  "require": {
    "php": ">=8.3",
    "ext-ctype": "*",
    "ext-iconv": "*",
    "ext-imagick": "*",
    "ext-json": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "ext-phalcon": "^5.8",
    "ext-sqlite3": "*",
    "ext-xml": "*",
    "php-ds/php-ds": "^1.5",
    "corneltek/getoptionkit": "^2.7.2",
    "diskerror/autoload": "dev-main",
    "diskerror/typed": "dev-master",
    "laminas/laminas-server": "^2.17"
  },
  "require-dev": {
    "phalcon/phalcon": ">=v5.0.x-dev",
    "phpunit/phpunit": "^10.3"
  },
  "suggest": {
    "ext-ds": "^1.5"
  },
  "config": {
    "optimize-autoloader": true
  },
  "autoload": {
    "psr-4": {
      "Application\\": "app/",
      "Application\\Structure\\": "app/struct/",
      "Library\\": "lib/",
      "": "./"
    }
  }
}
