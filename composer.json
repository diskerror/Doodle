{
    "name": "diskerror/doodle",
    "description": "A place for my stuff",
    "license": "MIT",
    "type": "project",
    "homepage": "https://rawsound.com",
	"repositories": [
		{
			"type": "path",
			"url": "../Typed"
		}
	],
    "require": {
        "php": "^8.2",
        "ext-imagick": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "corneltek/getoptionkit": "^2.7",
        "laminas/laminas-server": "^2.17",
        "diskerror/typed": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Library\\": "lib/",
            "Library\\Application\\": "lib/Application/",
            "Library\\Exception\\": "lib/Exception/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
