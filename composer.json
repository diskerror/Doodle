{
    "name": "diskerror/doodle",
    "description": "A place for my stuff",
    "license": "MIT",
    "type": "project",
    "homepage": "https://rawsound.com",
    "require": {
        "php": "^8.2",
        "corneltek/getoptionkit": "^2.7",
        "laminas/laminas-server": "^2.17"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Library\\": "lib/"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "minimum-stability": "dev",
    "prefer-stable": true

}
