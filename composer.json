{
  "name":          "diskerror/doodle",
  "description":   "A place for my stuff",
  "license":       "MIT",
  "type":          "project",
  "homepage":      "https://rawsound.com",
  "prefer-stable": true,
  "repositories":  [
    {
      "type":      "path",
      "url":       "../Typed",
      "canonical": false
    },
    {
      "type":      "vcs",
      "url":       "../Autoload",
      "canonical": false
    },
    {
      "type":      "vcs",
      "url":       "https://github.com/phalcon/phalcon-devtools",
      "canonical": false
    },
    {
      "type":      "path",
      "url":       "../pdfparser",
      "canonical": false
    }
  ],
  "require":       {
    "php":                    ">=8.3",
    "ext-iconv":              "*",
    "ext-mbstring":           "*",
    "ext-pcntl":              "*",
    "ext-pdo":                "*",
    "ext-phalcon":            "^5.8",
    "ext-sqlite3":            "*",
    "php-ds/php-ds":          "^1.7",
    "corneltek/getoptionkit": "2.7.3",
    "diskerror/autoload":     "dev-main",
    "diskerror/typed":        "dev-master",
    "laminas/laminas-server": "^2.18",
    "diskerror/pdfparser":    "dev-master"
  },
  "require-dev":   {
    "phalcon/phalcon": "v6.0.x-dev",
    "phpunit/phpunit": "^10.5.48"
  },
  "suggest":       {
    "ext-ds":      "^1.5",
    "ext-phalcon": "^5.9"
  },
  "config":        {
    "optimize-autoloader": true
  },
  "autoload":      {
    "psr-4": {
      "Application\\":            "app/",
      "Application\\Structure\\": "app/struct/",
      "Library\\":                "lib/",
      "":                         "./"
    }
  }
}
